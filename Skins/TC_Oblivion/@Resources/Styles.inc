[Variables]


;################################################################################################################################################################################
;FONTS===========================================================================================================================================================================
;################################################################################################################################################################################

Font=orbitron



;################################################################################################################################################################################
;COLORS==========================================================================================================================================================================
;################################################################################################################################################################################

White = 255,255,255
blue = 0,252,252
green = 26,144,158
red = 255,111,64
black = 0,0,0
tgreen = 135,216,236
dgreen = 86,117,125
yellow = 252,251,213
green_border1=86,117,125
green_border2=104,141,150
orange=191,144,56
orange2=227,163,45





;################################################################################################################################################################################
;TEXT-STYLES=====================================================================================================================================================================
;################################################################################################################################################################################

[TS_STD]
FontFace=#Font#
InlineSetting=CharacterSpacing | 1
InlinePattern=.*
StringCase=UPPER
Antialias=1


[TS_left]
StringAlign=Left

[TS_Center]
StringAlign=Center

[TS_right]
StringAlign=Right


[TS_Size6]
Fontsize=6

[TS_Size7]
Fontsize=7

[TS_Size8]
Fontsize=8

[TS_Size9]
Fontsize=9

[TS_Size10]
Fontsize=10

[TS_Size12]
Fontsize=12

[TS_Size14]
Fontsize=14

[TS_Size16]
Fontsize=16

[TS_Size18]
Fontsize=18

[TS_Size24]
Fontsize=24

[TS_Size30]
Fontsize=30


[TS_White]
FontColor=#White#

[TS_Blue]
FontColor=#blue#

[TS_Green]
FontColor=#green#

[TS_Red]
FontColor=#red#

[TS_Black]
FontColor=#black#

[TS_TGreen]
FontColor=#tgreen#

[TS_DGreen]
FontColor=#dgreen#

[TS_Yellow]
FontColor=#yellow#


[TS_Clip1]
ClipString=1

[TS_Clip2]
ClipString=2

;################################################################################################################################################################################
;OTHER-STYLES====================================================================================================================================================================
;################################################################################################################################################################################